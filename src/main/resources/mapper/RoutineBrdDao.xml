<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="kr.or.bodiary.routineBrd.dao.RoutineBrdDao">
    
   
	 <!-- 리스트 -->
    <select id="routineBoardList" resultType="kr.or.bodiary.routineBrd.dto.RoutineBrdDto">
		select u.user_nickname, b.routine_brd_title
		  from user as u
		  join routineboard as b
		    on u.user_email = b.user_email
	</select>
	
</mapper>
